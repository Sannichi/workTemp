<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="FindByTheme" preserve-order="true">
    
    <test name="Test - Change Inter MasterReseller Gateway to Adyen" preserve-order="true">
        <parameter name="gatewayName" value="Adyen" />     
        
        <classes>
			<class name="com.nymgo.AdminSignInCase">
                <methods>
                    <include name="signInAdminTest" />
                </methods>
            </class>
            <class name="com.nymgo.AdminResellersCase">
                <methods>
                    <include name="editInterMasterResellerGatewayAdminTest" />
                </methods>
            </class>
        </classes>
    </test>

    <test name="Test - BuyCredit Inter MasterReseller CAD Adyen MasterCard" preserve-order="true">
        <parameter name="paymentCurrency" value="CAD" />        
        <parameter name="cardType" value="MasterCard" />    
        <parameter name="gatewayName" value="Adyen" />     
          
        <classes>
            <class name="com.nymgo.DefaultCase">
                <methods>
                    <include name="setNymgoDefaultState" />
                </methods>
            </class>
            <class name="com.nymgo.SignInCase">
                <methods>
                    <include name="signInInterMasterResellerTest" />
                </methods>
            </class>
            <class name="com.nymgo.ViewAccountCases">
                <methods>
                    <include name="editInterResellerAccountTest" />
                </methods>
            </class>
            <class name="com.nymgo.BuyCreditCase">
                <methods>
                    <include name="buyCreditLoggedInterMasterResellerAdyenTest" />
                </methods>
            </class>
            <class name="com.nymgo.PayCreditCardCase">
                <methods>
                    <include name="payMasterCardAdyenPendingTest" />
                </methods>
            </class>
            <class name="com.nymgo.AdminSignInCase">
                <methods>
                    <include name="signInAdminTest" />
                </methods>
            </class>
            <class name="com.nymgo.AdminTransactionsCase">
                <methods>
                    <include name="acceptInterMasterResellerTransactionAdminTest" />
                </methods>
            </class>
            <class name="com.nymgo.VerifyBuyCreditCase">
                <methods>
                    <include name="verifyBuyCreditAcceptedLoggedInterMasterResellerTest" />
                </methods>
            </class>
        </classes>
    </test>
	
</suite>
