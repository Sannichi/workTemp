<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="FindByTheme" preserve-order="true">
    
    <parameter name="config.file.name" value="config.dev.properties" />
    <parameter name="params.file.name" value="params.properties" />
    
    <test name="Test - BuyCredut Normal User" preserve-order="true">
        <parameter name="paymentCurrency" value="AED" />        
        <parameter name="countryOfCredit" value="Belgium" />        
        <parameter name="cardType" value="American Express" />        
        <parameter name="gatewayName" value="GlobalCollect" />        
        <parameter name="currencyAmount" value="40" />        
        <parameter name="conversionRate" value="" />        
        <classes>
            <class name="nymgoAutomation.tests.testCases.SignInCase">
                <methods>
                    <include name="signInEuroNormalWhitelistUserTest" />
                </methods>
            </class>
            <class name="nymgoAutomation.tests.testCases.BuyCreditCase">
                <methods>
                    <include name="buyCreditLoggedNormalUserGlobalCollectTest" />
                    <include name="payAmericanExpressGlobalCollectPendingTest" />
                </methods>
            </class>
        </classes>
    </test>
    
</suite>
